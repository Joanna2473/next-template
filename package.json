{
  "name": "@salgnt/next-template",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18 <19",
    "pnpm": ">=7 <8"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "pnpx simple-git-hooks"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@types/node": "^18.11.6",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.2.2",
    "nano-staged": "^0.8.0",
    "next": "^13.0.0",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.8.4",
    "windicss": "^3.5.6",
    "windicss-webpack-plugin": "^1.7.7"
  },
  "nano-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "**/*": "prettier --ignore-path .gitignore --ignore-unknown --write"
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged",
    "preserveUnused": true
  }
}
